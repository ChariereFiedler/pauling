{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jest/globals": true,
        "node": true
    },
    "plugins": [
        "react",
        "jest"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-console": 1,
    },
    "globals": {
        "global": false,
        "Promise": false,
        "__DEV__": false,
        "sinon": true
    }
}
