{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jest/globals": true
    },
    "plugins": [
        "react",
        "jest"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-console": 1,
    },
    "globals": {
        "Promise": false,
        "__DEV__": false
    }
}
